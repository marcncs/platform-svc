<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="com.winsafe.mail.pojo.BatchCompleteMail"
		table="B_PROC_BATCH_COMPLETE_MAIL">
		<id name="mailId" type="long">
			<column name="MAIL_ID" />
			<generator class="sequence">
			<param name="sequence">MAIL_SEQ</param>
			</generator>
		</id>
<property column="MAIL_SENDER" name="mailSender" type="java.lang.String"/>
<property column="MAIL_FROM" name="mailFrom" type="java.lang.String"/>
<property column="MAIL_TO" name="mailTo" type="java.lang.String"/>
<property column="MAIL_CC" name="mailCc" type="java.lang.String"/>
<property column="MAIL_BCC" name="mailBcc" type="java.lang.String"/>
<property column="MAIL_SUBJECT" name="mailSubject" type="java.lang.String"/>
<property column="MAIL_BODY" name="mailBody" type="java.lang.String"/>
<property column="MAIL_ATTACHMENT" name="mailAttachment" type="java.lang.String"/>
<property column="CREATE_DATE" name="createDate" type="java.util.Date"/>
<property column="SENT_DATE" name="sentDate" type="java.util.Date"/>
<property column="IS_SENT" name="isSent" type="java.lang.Integer"/>
<property column="WRONG_TIMES" name="wrongTimes" type="java.lang.Integer"/>
<property column="MOBLIE_APPROVAL_ID" name="moblieApprovalId" type="java.lang.String"/>
<property column="MISS_MAIL_ADDRESS" name="missMailAddress" type="java.lang.String"/>
<property column="MAIL_DATE_TYPE" name="mailDateType" type="java.lang.String"/>

</class>
</hibernate-mapping>
