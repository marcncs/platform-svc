<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.winsafe.drp.dao.RegionItemInfo" table="S_BONUS_AREA">
		<id column="ID" name="id" type="java.lang.Integer">
			<generator class="sequence">
				<param name="sequence">S_BONUS_AREA_SEQ</param>
			</generator>
		</id>
		<property column="AREAID" name="areaId" type="java.lang.Integer"/>
		<property column="NAME_LOC" name="name" type="java.lang.String"/>
		<property column="NAME_EN" name="nameEn" type="java.lang.String"/>
		<property column="PARENTID" name="pId"  type="java.lang.Integer"/>
		<property column="RANK" name="rank" type="java.lang.Integer"/>
		<property column="MODIFIEDDATE" name="mdate" type="timestamp"/>
	</class>
</hibernate-mapping>
