<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 <class name="com.winsafe.drp.dao.Scanner" table='"SCANNER"' >
        <id name="id" column="ID" type="java.lang.Long">
           <generator class="assigned" />
        </id>
        <property name="model" type="java.lang.String">
        	<column name="MODEL" length="64" ></column>
        </property>
        <property name="osVersion" type="java.lang.String">
        	<column name="OSVERSION" length="64"></column>
        </property>
        <property name="status" type="java.lang.Long">
        	<column name="STATUS" length="19" />
        </property>
        <property name="installDate" type="java.util.Date">
        	<column name="INSTALLDATE" length="7"></column>
        </property>
        <property name="appVersion" type="java.lang.String">
        	<column name="APPVERSION" length="64" />
        </property>
        <property name="appVerUpDate" type="java.util.Date">
        	<column name="APPVERUPDATE" length="7"></column>
        </property>
        <property name="wHCode" type="java.lang.Long">
        	<column name="WHCODE" length="19" />
        </property>
        <property name="scannerName" type="java.lang.String">
        	<column name="SCANNERNAME" length="64"></column>
        </property>
        <property name="lastUpDate" type="java.util.Date">
        	<column name="LASTUPDATE" length="7" />
        </property>
        <property name="scannerImeiN" type="java.lang.String">
        	<column name="SCANNERIMEIN" length="19" />
        </property>

</class>
</hibernate-mapping>

