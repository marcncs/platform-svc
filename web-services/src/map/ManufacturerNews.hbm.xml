<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="com.winsafe.drp.dao.ManufacturerNews" table="manufacturer_news">
<id column="ID" name="id" type="java.lang.Integer">
<generator class="sequence"> 
<param name="sequence">BCS_RI_COMMON_SEQ</param>
</generator>
</id>
<property column="manufacturer_Id" name="manufacturerId" type="java.lang.Integer"/>
<property column="title" name="title" type="java.lang.String"/>
<property column="summary" name="summary" type="java.lang.String"/>
<property column="content" name="content" type="java.lang.String"/>
<property column="publish_Time" name="publishTime" type="java.util.Date"/>
<property column="pic_Url" name="picUrl" type="java.lang.String"/>
<property column="last_Modify_Time" name="lastModifyTime" type="java.util.Date"/>
<property column="is_Deleted" name="isDeleted" type="java.lang.Integer"/>
<property column="delete_Time" name="deleteTime" type="java.util.Date"/>
<property column="is_Show_Pic" name="isShowPic" type="java.lang.Integer"/>
<property column="viewCount" name="viewCount" type="java.lang.Integer"/>
</class> 
</hibernate-mapping>
