<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.winsafe.drp.dao.Query" table='"QUERY"'>
		<id name="id" column="ID" type="java.lang.String">
		<generator class="sequence">
			<param name="sequence">QUERY_SEQ</param>
		</generator>
		</id>
		<property name="proNumber" column="PRO_NUMBER" length="32" type="java.lang.String">
		</property>
		<property name="findDt" type="java.util.Date">
        	<column name="FINDDT" length="7" ></column>
        </property>
        <property name="telNumber" type="java.lang.String">
        	<column name="TELNUMBER" length="32"></column>
        </property>
        <property name="chkTrue" type="java.lang.Integer">
        	<column name="CHKTRUE" length="10" />
        </property>
        <property name="findType" type="java.lang.String">
        	<column name="FINDTYPE" length="32"></column>
        </property>
        <property name="compFlag" type="java.lang.String">
        	<column name="COMPFLAG" length="32" />
        </property>
        <property name="profile" type="java.lang.String">
        	<column name="PROFILE" length="32"></column>
        </property>
        <property name="remark" type="java.lang.String">
        	<column name="REMARK" length="32" />
        </property>
        <property name="areas" type="java.lang.String">
        	<column name="AREAS" length="32"></column>
        </property>
        <property name="queryNum" type="java.lang.Integer">
        	<column name="QUERYNUM" length="10"></column>
        </property>
        <property name="isFirstQuery" type="java.lang.Integer">
        	<column name="ISFIRSTQUERY" length="32"></column>
        </property>
         <property name="city" type="java.lang.String">
        	<column name="CITY" length="10"></column>
        </property>
        <property name="productid" type="java.lang.String">
        	<column name="PRODUCTID" length="32"></column>
        </property>
	</class>
</hibernate-mapping>