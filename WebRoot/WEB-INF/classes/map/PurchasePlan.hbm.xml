<property column="IsRatify" length="10" name="isratify" type="java.lang.Integer"/>
<property column="RatifyID" length="10" name="ratifyid" type="java.lang.Integer"/>
<property column="RatifyDate" length="23" name="ratifydate" type="java.util.Date"/>
<property column="IsComplete" length="10" name="iscomplete" type="java.lang.Integer"/>
<property column="Remark" length="256" name="remark" type="java.lang.String"/>
<property column="KeysContent" length="1024" name="keyscontent" type="java.lang.String"/>
</class>
</hibernate-mapping>
