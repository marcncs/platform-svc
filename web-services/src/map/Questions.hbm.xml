<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="com.winsafe.drp.dao.Questions" table="questions">
<id column="ID" name="id" type="java.lang.Integer">
<generator class="assigned"/>
</id>
<property column="Title" length="100" name="title" type="java.lang.String"/>
<property column="Content" length="1024" name="content" type="java.lang.String"/>
<property column="MakeOrganID" length="32" name="makeorganid" type="java.lang.String"/>
<property column="MakeDeptID" length="10" name="makedeptid" type="java.lang.Integer"/>
<property column="MakeID" length="10" name="makeid" type="java.lang.Integer"/>
<property column="MakeDate" length="23" name="makedate" type="java.util.Date"/>
</class>
</hibernate-mapping>
