<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="com.winsafe.drp.dao.ProductPicture" table="product_picture">
<id column="ID" name="id" type="java.lang.Integer">
<generator class="assigned"/>
</id>
<property column="ProductID" length="32" name="productid" type="java.lang.String"/>
<property column="PictureUrl" length="256" name="pictureurl" type="java.lang.String"/>
</class>
</hibernate-mapping>
