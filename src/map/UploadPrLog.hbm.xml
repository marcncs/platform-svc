<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="com.winsafe.drp.dao.UploadPrLog" table="upload_pr_log">
<id column="ID" name="id" type="java.lang.Integer">
<generator class="assigned"/>
</id>
<property column="IsDeal" length="10" name="isdeal" type="java.lang.Integer"/>
<property column="FileName" length="128" name="filename" type="java.lang.String"/>
<property column="FilePath" length="128" name="filepath" type="java.lang.String"/>
<property column="MakeID" length="10" name="makeid" type="java.lang.Integer"/>
<property column="MakeOrganID" length="50" name="makeorganid" type="java.lang.String"/>
<property column="MakeDeptID" length="10" name="makedeptid" type="java.lang.Integer"/>
<property column="MakeDate" length="23" name="makedate" type="java.util.Date"/>

<property column="logFilePath" length="128" name="logFilePath" type="java.lang.String"/>
<property column="errorCount" length="10" name="errorCount" type="java.lang.Integer"/>
<property column="totalCount" length="10" name="totalCount" type="java.lang.Integer"/>
<property column="fileHashCode" length="128" name="fileHashCode" type="java.lang.String"/>

</class>
</hibernate-mapping>
