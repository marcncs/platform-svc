<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="com.winsafe.drp.dao.UserGroup" table="User_Group">
<id column="ID" name="id" type="java.lang.Integer">
<generator class="sequence">
<param name="sequence">USER_GROUP_SEQ</param>
</generator>
</id> 
<property column="groupName" name="groupName" type="java.lang.String"/>
<property column="description" name="description" type="java.lang.String"/>
<property column="makeDate" name="makeDate" type="java.util.Date"/>
<property column="roleId" name="roleId" type="java.lang.Integer"/>
</class> 
</hibernate-mapping>
