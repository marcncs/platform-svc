<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="com.winsafe.notification.pojo.Notification" table="Notification">
<id column="id" name="id" type="java.lang.Long">
<generator class="native">
<!--<param name="seq_notification"></param>-->
</generator>
</id>
<property column="DELIVERYNO" length="32" name="deliveryNo" type="java.lang.String"/>
<property column="DELIVERYORDERTYPE" length="10" name="deliveryOrderType" type="java.lang.String"/>
<property column="LOGISTICCOMPANY" length="128" name="logisticCompany" type="java.lang.String"/>
<property column="DELIVERYPLACE" length="256" name="deliveryPlace" type="java.lang.String"/>
<property column="DELIVERYDATE" name="deliveryDate" type="java.util.Date"/>
<property column="ESTIMATEDATE" name="estimateDate" type="java.util.Date"/>
<property column="QUANTITY" length="10" name="quantity" type="java.lang.String"/>
<property column="CASESNO" name="casesNo" type="java.lang.Integer"/>
<property column="SHIPTOCODE" length="32" name="shipToCode" type="java.lang.String"/>
<property column="SHIPTOCOMPANY" length="256" name="shipToCompany" type="java.lang.String"/>
<property column="SHIPTOADDRESS" length="256" name="shipToAddress" type="java.lang.String"/>
<property column="CONSIGNEENAME" length="32" name="consigneeName" type="java.lang.String"/>
<property column="CONSIGNEEMOBILE" length="16" name="consigneeMobile" type="com.winsafe.hbm.entity.type.EncryptedMobile"/>
<property column="FILEPATH" length="256" name="filePath" type="java.lang.String"/>
<property column="FILENAME" length="128" name="fileName" type="java.lang.String"/>
<property column="LOGFILEPATH" length="256" name="logFilePath" type="java.lang.String"/>
<property column="SMSID" name="smsId" type="java.lang.Long"/>

</class>
</hibernate-mapping>
