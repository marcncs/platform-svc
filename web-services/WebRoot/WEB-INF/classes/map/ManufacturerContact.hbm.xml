<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="com.winsafe.drp.dao.ManufacturerContact" table="manufacturer_contact">
<id column="ID" name="id" type="java.lang.Integer">
<generator class="assigned"/>
</id>
<property column="manufacturerId" name="manufacturerId" type="java.lang.Integer"/>
<property column="title" name="title" type="java.lang.String"/>
<property column="tel" name="tel" type="java.lang.String"/>
<property column="createTime" name="createTime" type="java.util.Date"/>
<property column="lastModifyTime" name="lastModifyTime" type="java.util.Date"/>
<property column="isDeleted" name="isDeleted" type="java.lang.Boolean"/>
<property column="deleteTime" name="deleteTime" type="java.util.Date"/>
</class>
</hibernate-mapping>
