<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="com.winsafe.drp.dao.ProductPrice" table="product_price">
<id column="ID" name="id" type="java.lang.Integer">
<generator class="assigned"/>
</id>
<property column="ProductID" length="32" name="productid" type="java.lang.String"/>
<property column="UnitID" length="10" name="unitid" type="java.lang.Integer"/>
<property column="PolicyID" length="19" name="policyid" type="java.lang.Integer"/>
<property column="UnitPrice" length="18" name="unitprice" type="java.lang.Double"/>
</class>
</hibernate-mapping>
