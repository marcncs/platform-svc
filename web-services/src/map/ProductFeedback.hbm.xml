<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="com.winsafe.drp.dao.ProductFeedback" table="product_feedback">
<id column="ID" name="id" type="java.lang.Integer">
<generator class="sequence">
<param name="sequence">PRODUCT_FEEDBACK_SEQ</param> 
</generator>
</id>
<property column="productID" name="productId" type="java.lang.String"/> 
<property column="starScore" name="starScore" type="java.lang.Integer"/> 
<property column="content" name="content" type="java.lang.String"/> 
<!-- <property column="individualID" name="individualID" type="java.lang.Integer"/> -->
<property column="picUrl" name="picUrl" type="java.lang.String"/>
<property column="createTime" name="createTime" type="java.util.Date"/>
<property column="auditStatus" name="auditStatus" type="java.lang.Integer"/>
<property column="isreplied" name="replied" type="java.lang.Integer"/>
<property column="reply" name="reply" type="java.lang.String"/>
<property column="manufacturerAccountId" name="manufacturerAccountId" type="java.lang.Integer"/>
<property column="replyTime" name="replyTime" type="java.util.Date"/>
<property column="isDeleted" name="isDeleted" type="java.lang.Boolean"/>
<property column="deleteTime" name="deleteTime" type="java.util.Date"/>
<property column="productName" name="productName" type="java.lang.String"/> 
</class>
</hibernate-mapping>