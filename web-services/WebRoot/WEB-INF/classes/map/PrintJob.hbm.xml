<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="com.winsafe.sap.pojo.PrintJob" table="print_job" dynamic-update="true">
<id column="Print_job_ID" name="printJobId" type="java.lang.Integer">
<generator class="assigned"/>
</id>
<property column="Trans_order" length="20" name="transOrder" type="java.lang.String"/>
<property column="Trans_order_item" length="20" name="transOrderItem" type="java.lang.String"/>
<property column="plant_code" length="10" name="plantCode" type="java.lang.String"/>
<property column="plant_name" length="60" name="plantName" type="java.lang.String"/>
<property column="material_code" length="32" name="materialCode" type="java.lang.String"/>
<property column="material_Name" length="60" name="materialName" type="java.lang.String"/>
<property column="Shipper_Quantity" length="20" name="shipperQuantity" type="java.lang.String"/>
<property column="pack_size" length="20" name="packSize" type="java.lang.String"/>
<property column="process_order" length="20" name="processOrderNumber" type="java.lang.String"/>
<property column="Trans_order_date" name="transOrderDate" type="java.util.Date"/>
<property column="batch_number" length="10" name="batchNumber" type="java.lang.String"/>
<property column="GTIN_Number" length="10" name="GTINNumber" type="java.lang.String"/>
<property column="number_of_cases" name="numberOfCases" type="java.lang.Integer"/>
<property column="total_number" name="totalNumber" type="java.lang.Integer"/>
<property column="production_date" length="8" name="productionDate" type="java.lang.String"/>
<property column="packaging_date" length="8" name="packagingDate" type="java.lang.String"/>
<property column="expiry_date" length="8" name="expiryDate" type="java.lang.String"/>
<property column="printing_status" name="printingStatus" type="java.lang.Integer"/>
<property column="printing_date" name="printingDate" type="java.util.Date"/>
<property column="Create_Date" name="createDate" type="java.util.Date"/>
<property column="Create_user" name="createUser" type="java.lang.Integer"/>
<property column="upload_id" name="uploadId" type="java.lang.Integer"/>
<property column="primary_code_status" name="primaryCodeStatus" type="java.lang.Integer"/>
<property column="code_file_path" length="256" name="codeFilePath" type="java.lang.String"/>
<property column="product_id" length="32" name="productId" type="java.lang.String"/>
<property column="isdelete" name="isDelete" type="java.lang.Integer"/>
<property column="code_error_msg" length="256" name="codeErrorMsg" type="java.lang.String"/>
<property column="PRODUCTION_LINE" length="50" name="productionLine" type="java.lang.String"/>
<property column="CONFIRM_FLAG" name="confirmFlag" type="java.lang.Integer"/>
<property column="Shipper_Size" length="50" name="shipperSize" type="java.lang.String"/>
<property column="material_batch_no" length="5" name="materialBatchNo" type="java.lang.String"/>
<!-- 标签类型 -->  
<property column="labelType" length="5" name="labelType" type="java.lang.String"/>

<property column="codeType" name="codeType" type="java.lang.Integer"/>
<property column="linkMode" name="linkMode" type="java.lang.Integer"/>
<property column="syncStatus" name="syncStatus" type="java.lang.Integer"/>
<property column="syncFilePath" name="syncFilePath" type="java.lang.String"/>
<property column="cartonSeqStatus" name="cartonSeqStatus" type="java.lang.Integer"/>

</class>
</hibernate-mapping>
