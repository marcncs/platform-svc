<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="com.winsafe.drp.dao.OrganRole" table="organ_role">
<id column="ID" name="id" type="java.lang.Integer">
<generator class="assigned"/>
</id>

<property column="OrganID" length="32" name="organid" type="java.lang.String"/>
<property column="OName" length="256" name="oname" type="java.lang.String"/>
<property column="RoleID" length="10" name="roleid" type="java.lang.Integer"/>
<property column="RName" length="128" name="rname" type="java.lang.String"/>
</class>
</hibernate-mapping>
