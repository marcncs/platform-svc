<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="com.winsafe.drp.dao.RepositoryProduct" table="repository_product">
<id column="ID" name="id" type="java.lang.Long">
<generator class="assigned"/>
</id>
<property column="RID" length="32" name="rid" type="java.lang.String"/>
<property column="ProductID" length="32" name="productid" type="java.lang.String"/>
<property column="ProductName" length="128" name="productname" type="java.lang.String"/>
<property column="SpecMode" length="64" name="specmode" type="java.lang.String"/>
</class>
</hibernate-mapping>
