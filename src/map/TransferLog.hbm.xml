<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="com.winsafe.erp.pojo.TransferLog" table="TRANSFER_LOG">
<id column="ID" name="id" type="java.lang.Integer">
<generator class="sequence">
<param name="sequence">SEQ_TRANSFER_LOG</param>
</generator>
</id>
<property column="status" name="status" type="java.lang.Integer"/>
<property column="FILENAME" length="256" name="fileName" type="java.lang.String"/>
<property column="FILEPATH" length="256" name="filePath" type="java.lang.String"/>
<property column="FILETYPE" length="24" name="fileType" type="java.lang.Integer"/>
<property column="MAKEDATE" name="makeDate" type="java.util.Date"/>
<!--end modified by ryan.xi at 20150724-->
</class> 
</hibernate-mapping>
