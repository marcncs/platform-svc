<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="com.winsafe.drp.dao.RepositoryFile" table="repository_file">
<id column="ID" name="id" type="java.lang.Long">
<generator class="assigned"/>
</id>
<property column="RID" length="32" name="rid" type="java.lang.String"/>
<property column="Title" length="256" name="title" type="java.lang.String"/>
<property column="FilePath" length="256" name="filepath" type="java.lang.String"/>
</class>
</hibernate-mapping>
