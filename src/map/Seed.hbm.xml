<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="com.winsafe.sap.pojo.Seed" table="seed">
<id column="id" name="id" type="java.lang.Integer">
<generator class="sequence">
<param name="sequence">SEED_SEQ</param>
</generator>
</id>
<property column="type" name="type" type="java.lang.Integer"/>
<property column="seed" name="seed" type="java.lang.String"/>
<property column="currentValue" name="currentValue" type="java.lang.Long"/>
<property column="isUsed" name="isUsed" type="java.lang.Integer"/>
<property column="makeDate" name="makeDate" type="java.util.Date"/>
<property column="year" name="year" type="java.lang.Integer"/>
</class>
</hibernate-mapping> 
