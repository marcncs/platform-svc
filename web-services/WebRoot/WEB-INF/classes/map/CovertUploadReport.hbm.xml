<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="com.winsafe.sap.pojo.CovertUploadReport" table="COVERT_UPLOAD_REPORT">
<id column="id" name="id" type="java.lang.Long">
<generator class="sequence">
<param name="sequence">seq_covert_upload_report</param>
</generator>
</id>
<property column="ERROR_COUNT" name="errorCount" type="java.lang.Integer"/>
<property column="TOTAL_COUNT" name="totalCount" type="java.lang.Integer"/>
<property column="LINE_NO" length="50" name="lineNo" type="java.lang.String"/>
<property column="RECODE_TYPE" name="recodeType" type="java.lang.Integer"/>
<property column="PRINT_DATE" length="10" name="printDate" type="java.lang.String"/>
<property column="UPLOAD_PR_ID" name="uploadPrId" type="java.lang.Integer"/>
<property column="UPLOAD_DATE" name="uploadDate" type="java.util.Date"/>
<property column="UPLOAD_USER" name="uploadUser" type="java.lang.Integer"/>
<property column="MATERIAL_CODE" length="8" name="materialCode" type="java.lang.String"/>
<property column="PRODUCT_NAME" length="128" name="productName" type="java.lang.String"/>
<property column="BATCH" length="32" name="batch" type="java.lang.String"/>
<property column="PRODUCT_ID" length="32" name="productId" type="java.lang.String"/>
</class>
</hibernate-mapping>
