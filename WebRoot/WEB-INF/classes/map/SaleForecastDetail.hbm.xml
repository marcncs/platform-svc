<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="com.winsafe.drp.dao.SaleForecastDetail" table="sale_forecast_detail">
<id column="ID" name="id" type="java.lang.Integer">
<generator class="assigned"/>
</id>
<property column="SID" length="10" name="sid" type="java.lang.Integer"/>
<property column="ProductID" length="32" name="productid" type="java.lang.String"/>
<property column="ProductName" length="128" name="productname" type="java.lang.String"/>
<property column="SpecMode" length="64" name="specmode" type="java.lang.String"/>
<property column="Quantity" length="18" name="quantity" type="java.lang.Double"/>
<property column="SubSum" length="18" name="subsum" type="java.lang.Double"/>
<property column="UnitID" length="10" name="unitid" type="java.lang.Integer"/>
<property column="UnitPrice" length="18" name="unitprice" type="java.lang.Double"/>
</class>
</hibernate-mapping>
