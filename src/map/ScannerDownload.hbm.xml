<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 <class name="com.winsafe.drp.dao.ScannerDownload" table='ScannerDownload' >
 
        <id name="id" column="ID" type="java.lang.Integer">
           <generator class="assigned" />
        </id>
        <property name="imei" type="java.lang.String">
        	<column name="imei" length="255" ></column>
        </property>
        <property name="downloadMd5" type="java.lang.String">
        	<column name="downloadMd5" length="64"></column>
        </property>
</class>
</hibernate-mapping>

