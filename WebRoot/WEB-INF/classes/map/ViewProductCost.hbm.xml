<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="com.winsafe.drp.dao.ViewProductCost" table="view_product_cost">
<id column="warehouseid" name="warehouseid" type="java.lang.String">
<generator class="assigned"/>
</id>
<property column="productid" length="32" name="productid" type="java.lang.String"/>
<property column="productname" length="128" name="productname" type="java.lang.String"/>
<property column="batch" length="32" name="batch" type="java.lang.String"/>
<property column="unitid" length="10" name="unitid" type="java.lang.Integer"/>
<property column="cost" length="20" name="cost" type="java.lang.Double"/>
<property column="mixcost" length="20" name="mixcost" type="java.lang.Double"/>
<property column="xquantity" length="20" name="xquantity" type="java.lang.Double"/>
<property column="mixquantity" length="20" name="mixquantity" type="java.lang.Double"/>
</class>
</hibernate-mapping>
