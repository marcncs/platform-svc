<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="com.winsafe.drp.dao.PopularProduct" table="popular_product">
<id column="ID" name="id" type="java.lang.String">
<generator class="assigned"/>
</id>
<property column="Name" length="255" name="name" type="java.lang.String"/> 
<property column="Name_Eng" length="255" name="nameEng" type="java.lang.String"/>
<property column="Alias" length="255" name="alias" type="java.lang.String"/>
<property column="Audit_Status" length="3" name="auditStatus" type="java.lang.Integer"/>
<property column="Listed_Status" length="3" name="listedStatus" type="java.lang.Integer"/>
<property column="Manufacturer_ID" length="11" name="manufacturerId" type="java.lang.Integer"/>
<property column="Group1_ID" length="11" name="group1Id" type="java.lang.Integer"/>
<property column="Group2_ID" length="11" name="group2Id" type="java.lang.Integer"/>
<property column="Slogan" length="255" name="slogan" type="java.lang.String"/>
<property column="Component" length="255" name="component" type="java.lang.String"/>
<property column="Certification" length="255" name="certification" type="java.lang.String"/>
<property column="Anti_Forgery_Code" length="255" name="antiForgeryCode" type="java.lang.String"/>
<property column="Pic_Url" length="255" name="picUrl" type="java.lang.String"/>
<property column="Content" length="4000" name="content" type="java.lang.String"/>
<property column="Rank" length="11" name="rank" type="java.lang.Integer"/>
<property column="Label_Url" length="255" name="labelUrl" type="java.lang.String"/>
<property column="Poster_Url" length="255" name="posterUrl" type="java.lang.String"/>
<property column="Sku" length="255" name="sku" type="java.lang.String"/>
<property column="Tel" length="255" name="tel" type="java.lang.String"/>
<property column="Create_Time" name="createTime" type="java.util.Date"/>
<property column="Last_Modify_Time" name="lastModifyTime" type="java.util.Date"/>
<property column="Is_Deleted" length="3" name="isDeleted" type="java.lang.Integer"/>
<property column="Delete_Time" name="deleteTime" type="java.util.Date"/>
</class>
</hibernate-mapping>