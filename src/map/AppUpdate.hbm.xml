<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="com.winsafe.app.pojo.AppUpdate" table="APP_UPDATE">
<id column="ID" name="id" type="java.lang.Long">
<generator class="native"/>
</id>
<property column="APPNAME" length="128" name="appName" type="java.lang.String"/>
<property column="PUBLISHNAME" length="128" name="publishName" type="java.lang.String"/>
<property column="APPVERSION" length="64" name="appVersion" type="java.lang.String"/>
<property column="PUBLISHDATE" name="publishDate" type="java.util.Date"/>
<property column="UPDATELOG" length="512" name="updateLog" type="java.lang.String"/>
<property column="APPURL" length="512" name="appUrl" type="java.lang.String"/>
<property column="PUBLISHER" name="publisher" type="java.lang.Integer"/>
<property column="FILEPATH" length="512" name="filePath" type="java.lang.String"/>
<property column="DOWNLOADCOUNT" name="downloadCount" type="java.lang.Integer"/>
</class>
</hibernate-mapping>
