<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.winsafe.drp.dao.Region" table="region">
		<id column="ID" name="id" type="java.lang.Long">
			<generator class="sequence">
				<param name="sequence">region_seq</param>
			</generator>
		</id>
		<property column="RegionCode" length="64" name="regioncode"
			type="java.lang.String" />
		<property column="SortName" length="64" name="sortname"
			type="java.lang.String" />
		<property column="parentid" length="64" name="parentid"
			type="java.lang.String"></property>
		<property column="parentname" length="64" name="parentname"
			type="java.lang.String"></property>
		<property column="userid" length="64" name="userid" type="java.lang.String"></property>

		<property column="username" length="64" name="username"
			type="java.lang.String"></property>

		<property column="Remark" length="128" name="remark"
			type="java.lang.String" />

		<property column="typeid" name="typeid" length="32"
			type="java.lang.String"></property>
		<property column="typename" name="typename" length="64"
			type="java.lang.String"></property>
		<property column="parentuserid" name="parentuserid" length="64"
			type="java.lang.String"></property>
		<property column="parentusername" name="parentusername"
			length="64" type="java.lang.String"></property>



	</class>
</hibernate-mapping>
