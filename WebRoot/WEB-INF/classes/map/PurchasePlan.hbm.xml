<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="com.winsafe.drp.dao.PurchasePlan" table="purchase_plan">
<id column="ID" name="id" type="java.lang.String">
<generator class="assigned"/>
</id>
<property column="BillNo" length="32" name="billno" type="java.lang.String"/>
<property column="PurchaseSort" length="10" name="purchasesort" type="java.lang.Integer"/>
<property column="PlanDate" length="23" name="plandate" type="java.util.Date"/>
<property column="PlanDept" length="10" name="plandept" type="java.lang.Integer"/>
<property column="PlanID" length="10" name="planid" type="java.lang.Integer"/>
<property column="TotalSum" length="18" name="totalsum" type="java.lang.Double"/>
