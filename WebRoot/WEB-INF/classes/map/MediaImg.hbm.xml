<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="com.winsafe.drp.dao.MediaImage" table="media_image">
<id column="ID" name="id" type="java.lang.Integer">
<generator class="sequence">
<param name="sequence">MEDIA_IMAGE_SEQ</param>
</generator> 
</id>
<property column="MANUFACTURER_ID" name="manufacturerId" type="java.lang.Integer"/>
<property column="title" name="title" type="java.lang.String"/>
<property column="url" name="url" type="java.lang.String"/>
<property column="GROUP_ID" name="groupId" type="java.lang.Integer"/>
<property column="IMG_SIZE" name="size" type="java.lang.Long"/>
<property column="CREATE_TIME" name="createTime" type="java.util.Date"/>
<property column="LAST_MODIFY_TIME" name="lastModifyTime" type="java.util.Date"/>
<property column="IS_DELETED" name="isDeleted" type="java.lang.Integer"/>
<property column="DELETE_TIME" name="deleteTime" type="java.util.Date"/>
<property column="HASH" name="hash" type="java.lang.String"/>
<property column="MIMETYPE" name="mimeType" type="java.lang.String"/>
<property column="PATH" name="path" type="java.lang.String"/>
</class>
</hibernate-mapping>