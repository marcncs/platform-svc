<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="com.winsafe.drp.dao.PhoneBook" table="phone_book">
<id column="ID" name="id" type="java.lang.Integer">
<generator class="assigned"/>
</id>
<property column="Name" length="64" name="name" type="java.lang.String"/>
<property column="Sex" length="10" name="sex" type="java.lang.Integer"/>
<property column="Phone" length="26" name="phone" type="java.lang.String"/>
<property column="Mobile" length="26" name="mobile" type="java.lang.String"/>
<property column="Email" length="64" name="email" type="java.lang.String"/>
<property column="Qq" length="24" name="qq" type="java.lang.String"/>
<property column="Msn" length="64" name="msn" type="java.lang.String"/>
<property column="Birthday" length="23" name="birthday" type="java.util.Date"/>
<property column="Addr" length="256" name="addr" type="java.lang.String"/>
<property column="Remark" length="256" name="remark" type="java.lang.String"/>
<property column="SortID" length="10" name="sortid" type="java.lang.Integer"/>
<property column="UserID" length="10" name="userid" type="java.lang.Integer"/>
</class>
</hibernate-mapping>
